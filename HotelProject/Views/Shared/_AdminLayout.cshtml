
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>AdminLTE 3 | Dashboard</title>
    <link href="~/Admin/assets/plugins/datatables/plugins/bootstrap/dataTables.bootstrap4.min.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.2.1/css/all.min.css" integrity="sha512-MV7K8+y+gLIBoVD59lQIYicR65iaqukzvf/nwasF0nqhPay5w/9lJmVM2hMDcnK1OnMGCdVK+iQrJ7lzPJQd1w==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <!-- Google Font: Source Sans Pro -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
    <!-- Font Awesome -->
    <link rel="stylesheet" href="~/AdminLTE/AdminLTE-master/plugins/fontawesome-free/css/all.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Tempusdominus Bootstrap 4 -->
    <link rel="stylesheet" href="~/AdminLTE/AdminLTE-master/plugins/tempusdominus-bootstrap-4/css/tempusdominus-bootstrap-4.min.css">
    <!-- iCheck -->
    <link rel="stylesheet" href="~/AdminLTE/AdminLTE-master/plugins/icheck-bootstrap/icheck-bootstrap.min.css">
    <!-- JQVMap -->
    <link rel="stylesheet" href="~/AdminLTE/AdminLTE-master/plugins/jqvmap/jqvmap.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/AdminLTE/AdminLTE-master/dist/css/adminlte.min.css">
    <!-- overlayScrollbars -->
    <link rel="stylesheet" href="~/AdminLTE/AdminLTE-master/plugins/overlayScrollbars/css/OverlayScrollbars.min.css">
    <!-- Daterange picker -->
    <link rel="stylesheet" href="~/AdminLTE/AdminLTE-master/plugins/daterangepicker/daterangepicker.css">
    <!-- summernote -->
    <link rel="stylesheet" href="~/AdminLTE/AdminLTE-master/plugins/summernote/summernote-bs4.min.css">
</head>

<body class="hold-transition sidebar-mini layout-fixed">
    <div class="wrapper">
        <!-- Navbar -->
        <nav class="main-header navbar navbar-expand navbar-white navbar-light">
            <!-- Left navbar links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                </li>
            </ul>

            <!-- Right navbar links -->
        </nav>
        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <!-- Brand Logo -->
            <a href="~/Admin/Admin/AdminAnasayfa" class="brand-link">
                <img src="~/admin/dist/img/AdminLTELogo.png" alt="AdminLTE Logo" class="brand-image img-circle elevation-3" style="opacity: .8">
                <span class="brand-text font-weight-light">Otel Admin</span>
            </a>

            <!-- Sidebar -->
            <div class="sidebar">
                <!-- Sidebar user panel (optional) -->
                <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                    <div class="image">
                        <img src="~/admin/dist/img/user2-160x160.jpg" class="img-circle elevation-2" alt="User Image">
                    </div>
                    <div class="info">
                        <a href="#" class="d-block">Open Yazılım</a>
                    </div>
                </div>

                <!-- SidebarSearch Form -->
                <div class="form-inline">
                    <div class="input-group" data-widget="sidebar-search">
                        <input class="form-control form-control-sidebar" type="search" placeholder="Ara" aria-label="Search">
                        <div class="input-group-append">
                            <button class="btn btn-sidebar">
                                <i class="fas fa-search fa-fw"></i>
                            </button>
                        </div>
                    </div>
                </div>

                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <!-- Add icons to the links using the .nav-icon class
                             with font-awesome or any other icon font library -->

                        <li class="nav-item">
                            <a href="~/Admin/Admin/AdminAnasayfa" class="nav-link">
                                <i class="nav-icon fas fa-columns"></i>
                                <p>
                                    Ana Sayfa
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Admin/Rezervasyon/Index" class="nav-link">
                                <i class="nav-icon fa-solid fa-suitcase-rolling"></i>
                                <p>
                                    Rezervasyonlar
                                    <i class="right fas fa-angle-left"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="/Admin/Rezervasyon/Index" class="nav-link">
                                        <i class="fa-solid fa-arrow-right"></i>
                                        <p>Mevcut Rezervasyonlar</p>
                                    </a>
                                </li>
                            </ul>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="/Admin/Rezervasyon/EskiRezervasyon" class="nav-link">
                                        <i class="fa-solid fa-arrow-right"></i>
                                        <p>Eski Rezervasyonlar</p>
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <li class="nav-item">
                            <a href="#" class="nav-link">
                                <i class="nav-icon far fa-envelope"></i>
                                <p>
                                    Odalar
                                    <i class="fas fa-angle-left right"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="/Admin/Odalar/Odalar" class="nav-link">
                                        <i class="fa-solid fa-arrow-right"></i>
                                        <p>Odalar</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/Admin/OdaTipleri/OdaTipleri" class="nav-link">
                                        <i class="fa-solid fa-arrow-right"></i>
                                        <p>Oda Tipleri</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/Admin/Pansiyonlar/Pansiyonlar" class="nav-link">
                                        <i class="fa-solid fa-arrow-right"></i>
                                        <p>Pansiyonlar</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/Admin/OdaOzellikleri/OdaOzellikleri" class="nav-link">
                                        <i class="fa-solid fa-arrow-right"></i>
                                        <p>Oda Özellikleri</p>
                                    </a>
                                </li>
                            </ul>
                        </li>
                        <li class="nav-item">
                            <a href="/Admin/Musteriler/Musteriler" class="nav-link">
                                <i class="nav-icon fas fa-book"></i>
                                <p>
                                    Müşteriler
                                    <i class="fas fa-angle-left right"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview">
                                <li class="nav-item">
                                    <a href="/Admin/Musteriler/Musteriler" class="nav-link">
                                        <i class="fa-solid fa-arrow-right"></i>
                                        <p>Müşteriler</p>
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a href="/Admin/Musteriler/YeniMusteri" class="nav-link">
                                        <i class="fa-solid fa-arrow-right"></i>
                                        <p>Yeni Müşteri</p>
                                    </a>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>


        <div class="content-wrapper">
            @RenderBody()
        </div>
        <footer class="main-footer">
            @*<div class="float-right d-none d-sm-block">

                </div>*@
            <strong>Copyright &copy; @DateTime.Today.Year <a href="https://openyazilim.com/open/">OPEN YAZILIM</a></strong> Tüm hakları saklıdır.
        </footer>
        <aside class="control-sidebar control-sidebar-dark">
            <!-- Control sidebar content goes here -->
        </aside>
        <!-- /.control-sidebar -->
    </div>


    <script src="~/AdminLTE/AdminLTE-master/plugins/datatables/jquery.dataTables.min.js"></script>
    <script src="~/AdminLTE/AdminLTE-master/plugins/datatables-bs4/js/dataTables.bootstrap4.min.js"></script>
    <script src="~/AdminLTE/AdminLTE-master/plugins/datatables-responsive/js/dataTables.responsive.min.js"></script>
    <script src="~/AdminLTE/AdminLTE-master/plugins/datatables-responsive/js/responsive.bootstrap4.min.js"></script>
    <script src="~/AdminLTE/AdminLTE-master/plugins/datatables-buttons/js/dataTables.buttons.min.js"></script>
    <script src="~/AdminLTE/AdminLTE-master/plugins/datatables-buttons/js/buttons.bootstrap4.min.js"></script>
    <script src="~/AdminLTE/AdminLTE-master/plugins/jszip/jszip.min.js"></script>
    <script src="~/AdminLTE/AdminLTE-master/plugins/pdfmake/pdfmake.min.js"></script>
    <script src="~/AdminLTE/AdminLTE-master/plugins/pdfmake/vfs_fonts.js"></script>
    <script src="~/AdminLTE/AdminLTE-master/plugins/datatables-buttons/js/buttons.html5.min.js"></script>
    <script src="~/AdminLTE/AdminLTE-master/plugins/datatables-buttons/js/buttons.print.min.js"></script>
    <script src="~/AdminLTE/AdminLTE-master/plugins/datatables-buttons/js/buttons.colVis.min.js"></script>
    <!-- jQuery -->
    <script src="~/AdminLTE/AdminLTE-master/plugins/jquery/jquery.min.js"></script>
    <!-- jQuery UI 1.11.4 -->
    <script src="~/AdminLTE/AdminLTE-master/plugins/jquery-ui/jquery-ui.min.js"></script>
    <!-- Resolve conflict in jQuery UI tooltip with Bootstrap tooltip -->
    <script>$.widget.bridge('uibutton', $.ui.button)</script>
    <!-- Bootstrap 4 -->
    <script src="~/AdminLTE/AdminLTE-master/plugins/bootstrap/js/bootstrap-datepicker.js"></script>
    <script src="~/AdminLTE/AdminLTE-master/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- ChartJS -->
    <script src="~/AdminLTE/AdminLTE-master/plugins/chart.js/Chart.min.js"></script>
    <!-- Sparkline -->
    <script src="~/AdminLTE/AdminLTE-master/plugins/sparklines/sparkline.js"></script>
    <!-- JQVMap -->
    <script src="~/AdminLTE/AdminLTE-master/plugins/jqvmap/jquery.vmap.min.js"></script>
    <script src="~/AdminLTE/AdminLTE-master/plugins/jqvmap/maps/jquery.vmap.usa.js"></script>
    <!-- jQuery Knob Chart -->
    <script src="~/AdminLTE/AdminLTE-master/plugins/jquery-knob/jquery.knob.min.js"></script>
    <!-- daterangepicker -->
    <script src="~/AdminLTE/AdminLTE-master/plugins/moment/moment.min.js"></script>
    <script src="~/AdminLTE/AdminLTE-master/plugins/daterangepicker/daterangepicker.js"></script>
    <!-- Tempusdominus Bootstrap 4 -->
    <script src="~/AdminLTE/AdminLTE-master/plugins/tempusdominus-bootstrap-4/js/tempusdominus-bootstrap-4.min.js"></script>
    <!-- Summernote -->
    <script src="~/AdminLTE/AdminLTE-master/plugins/summernote/summernote-bs4.min.js"></script>
    <!-- overlayScrollbars -->
    <script src="~/AdminLTE/AdminLTE-master/plugins/overlayScrollbars/js/jquery.overlayScrollbars.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/AdminLTE/AdminLTE-master/dist/js/adminlte.js"></script>

    <script src="~/Admin/assets/plugins/sweet-alert/sweetalert2.all.min.js"></script>

    @if (TempData["success"] != null)
    {
        <script>
            Swal.fire({
                icon: 'success',
                title: 'Başarılı',
                confirmButtonText:'Tamam',
                text: '@Html.Raw(TempData["success"])'
            })
        </script>
    }
    @if (TempData["error"] != null)
    {
        <script>
            Swal.fire({
                icon: 'error',
                title: 'Hata',
                confirmButtonText:'Tamam',
                text: '@Html.Raw(TempData["error"])'
            })
        </script>
    }
    @if (TempData["warning"] != null)
    {
        <script>
            Swal.fire({
                icon: 'warning',
                title: 'Dikkat',
                confirmButtonText:'Tamam',
                text: '@Html.Raw(TempData["warning"])'
            })
        </script>
    }
    @if (TempData["info"] != null)
    {
        <script>
            Swal.fire({
                icon: 'info',
                title: 'Bilgi',
                confirmButtonText:'Tamam',
                text:'@Html.Raw(TempData["info"])'
            })
        </script>
    }
    



    <!-- AdminLTE for demo purposes -->
    <!--<script src="~/AdminLTE/AdminLTE-master/dist/js/demo.js"></script>-->
    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    <!--<script src="~/AdminLTE/AdminLTE-master/dist/js/pages/dashboard.js"></script>-->






</body>
</html>


